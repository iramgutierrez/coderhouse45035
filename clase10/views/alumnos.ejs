<!DOCTYPE html>
  <html lang="en">
  <head>
    <%- include('./partials/head') %>
  </head>
  <body>
    <div class="container">
      <header>
        <%- include('./partials/header', { current: 'alumnos' }) %>
      </header>

      <main>
        <div class="jumbotron">
          <h1>Curso de Programación Backend</h1>
          <h2>Alumnos de la comisión <%= comision %></h2>
          <ul>
            <% alumnos.forEach(alumno => { %>
              <li><%= alumno.nombre %> <%= alumno.apellido %></li>
            <% }) %>
          </ul>
        </div>
      </main>
      <%- include('./partials/footer') %>
    </div>
  </body>
</html>